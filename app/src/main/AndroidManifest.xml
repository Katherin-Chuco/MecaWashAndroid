<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.mecawash">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name="com.example.mecawash.controllers.activities.ReservationServiceActivity"
                android:label="@string/title_activity_reservation_service"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name="com.example.mecawash.controllers.activities.ServiceClientActivity"
                android:label="@string/title_activity_service_client"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name="com.example.mecawash.controllers.activities.ProviderClientActivity"
                android:label="@string/title_activity_provider_client"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name="com.example.mecawash.controllers.activities.RequestStatusFinishActivity">
        </activity>
        <activity android:name="com.example.mecawash.controllers.activities.RequestStatusAcceptActivity">
        </activity>
        <activity android:name="com.example.mecawash.controllers.activities.RequestStatusActiveActivity">
        </activity>
        <activity android:name="com.example.mecawash.controllers.activities.HomeProviderActivity">
        </activity>
        <activity android:name="com.example.mecawash.controllers.activities.LoginCustomerActivity">
        </activity>
        <activity android:name="com.example.mecawash.controllers.activities.LoginProviderActivity">
        </activity>
        <activity android:name="com.example.mecawash.controllers.activities.LocalActivity">
        </activity>
        <activity android:name="com.example.mecawash.controllers.activities.ProviderActivity">
        </activity>
        <activity android:name="com.example.mecawash.controllers.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>